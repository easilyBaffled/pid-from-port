		"get-port": "^4.0.0",
		"nyc": "^12.0.2",
		"pre-commit": "^1.2.2",
		"testcheck": "^1.0.0-rc.2",
	  	"type-detect": "^4.0.8",
		"xo": "0.21.1"
	}
